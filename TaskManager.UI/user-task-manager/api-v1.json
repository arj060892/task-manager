{
  "$schema": "./node_modules/ng-swagger-gen/ng-swagger-gen-schema.json",
  "swagger": "swagger.json",
  "output": "src/app/shared/utmapi/v1",
  "prefix": "UtmApiV1",
  "ignoreUnusedModels": true,
  "minParamsForContainer": 2,
  "sortParams": "none",
  "defaultTag": "Api",
  "removeStaleFiles": true,
  "modelIndex": true,
  "serviceIndex": true,
  "apiModule": true,
  "enumModule": true,
  "generateExamples": true,
  "camelCase": true,
  "customFileSuffix": {
    "model": "",
    "example": ".example",
    "service": ".service"
  },
  "timeout": 20000,
  "skipProxySetup": false
}
